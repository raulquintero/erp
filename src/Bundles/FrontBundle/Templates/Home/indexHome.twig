<style>

    .bg {
    position: absolute;
    margin-left: auto;
    margin-top: auto
    }
    .background-wrap {
        width: 100%;
        height: 100%;
        position: absolute;
        top: 40px;
        left: 0;
        z-index: -1;
        overflow: hidden;
    }
    .background {
        background: url('/img/wall_pos.jpg');
        background-size: cover;
        filter: blur(10px);
        height: 105%;
        position: relative;
        width: 105%;
        right: -2.5%;
        left: -2.5%;
        top: -2.5%;
        bottom: -2.5%;
    }


    #left {
        width:0px;
    }
    #main {
        left: 0px;
        margin-left: 0px;
        padding-bottom: 0px;
    }
    .wallpaper {
        width: 100%;
    }
    .pricing {
        min-width: 170px;
    }
    .logo {
        width: 150px;
        height: 119.5px
    }
</style>

    
